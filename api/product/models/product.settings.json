{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "category": {
      "model": "category"
    },
    "price": {
      "type": "decimal"
    },
    "merchantPrice": {
      "type": "decimal"
    },
    "displayName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "valid": {
      "type": "boolean",
      "default": true
    },
    "merchant": {
      "model": "merchant"
    }
  }
}
