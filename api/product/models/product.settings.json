{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "index": true
    },
    "price": {
      "type": "decimal",
      "index": true
    },
    "displayName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "merchant": {
      "private": true,
      "model": "merchant"
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Vinaphone",
        "Mobifone",
        "Viettel",
        "Vietnamobile",
        "Gmobile",
        "iTelecom"
      ],
      "index": true
    }
  }
}
